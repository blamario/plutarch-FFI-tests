digraph Deposit {
  rankdir = LR;

  wallet_in
    [ shape=record
    , label=
        "{{ Client wallet
          | EthAddress: abcdef0123456789
          | Amount: 10
          | TokenInfo: (Token1, Token2)
         }}"
    ];

  wallet_out
    [ shape=record
    , label=
        "{{ Server Controlled Script Address
          | Token1: 10
          | Token2: 10
         }}"
    ];

  tx [label="Tx"];

  wallet_in -> tx;
  tx -> wallet_out;
}
